<section class="ticket-new">

  <header class="page-header">
    <h1>F.A.Q</h1>
  </header>

  <div class="well">
    <%= link_to 'All F.A.Qs', [:qas], class: 'btn btn-info' %>
  </div>

  <div class="row">

        <div class="form-group">

          <h3> Problem </h3>
          <%= @qa.problem.html_safe %>
          <hr />
          <h3> Solution </h3>
          <%= @qa.solution.html_safe %>
          <hr />

        </div>
  </div>

</section>
