<tr>
  <td>
  <%= QaCategory.find_by_id(qa.qa_category_id).code %>
  </td>
  <td>
  <%= qa.problem.html_safe %>
  </td>
  <td>
  <%= qa.solution.html_safe %>
  </td>
  <td>
    <%= link_to fa_icon('edit'), edit_admin_qa_path(qa), class: 'btn btn-sm btn-primary' %>
  </td>
  <td>
    <%= link_to fa_icon('trash'), [:admin, qa], class: 'btn btn-sm btn-danger', method: :delete, data: { confirm: "Are you sure?" } %>
  </td>

</tr>