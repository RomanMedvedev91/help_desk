<tr>
  <td>
    <h4><%= user_type.code %></h4>
  </td>
  <td>
    <h4><%= user_type.description.html_safe %></h4>
  </td>
  <td>
    <%= link_to fa_icon('edit'), edit_admin_user_type_path(user_type), class: 'btn btn-sm btn-primary' %>
  </td>
  <td>
    <%= link_to fa_icon('trash'), [:admin, user_type], class: 'btn btn-sm btn-danger', method: :delete, data: { confirm: "Are you sure?" } %>
  </td>

</tr>